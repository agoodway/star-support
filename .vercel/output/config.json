{
	"version": 3,
	"routes": [
		{
			"src": "^/\\.well-known(?:/.*)?$"
		},
		{
			"src": "^/((?:[^/]+/)*[^/\\.]+)$",
			"headers": {
				"Location": "/$1/"
			},
			"status": 308
		},
		{
			"src": "^/((?:[^/]+/)*[^/]+\\.\\w+)/$",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/$",
			"dest": "_render"
		},
		{
			"src": "^/_image/$",
			"dest": "_render"
		},
		{
			"src": "^/api/star-support/chat/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}